{
  "name": "dawdler-react-project-template",
  "version": "0.0.1",
  "description": "react project template By:dawdler",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production && npm run clean && webpack --env.mode=production --mode production --progress --config webpack.config.js",
    "dev": "cross-env NODE_ENV=development && npm run clean && webpack-dev-server --mode development --progress --config webpack.config.js",
    "upgrade": "npm update",
    "precommit": "lint-staged",
    "lint": "prettier --write \"src/**/*.js\" && eslint_d --fix src",
    "test": "jest __tests__/tests",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "pre-commit": "lint",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lijinke666/react-project-template.git"
  },
  "keywords": [
    "dawdler",
    "webpack",
    "webpack2",
    "webpack3",
    "webpack4",
    "HTML5",
    "CSS3",
    "html5",
    "file",
    "uplaod",
    "react",
    "redux",
    "react-redux",
    "yarn",
    "npm",
    "pc",
    "web",
    "es6",
    "es7",
    "template",
    "babel",
    "cli",
    "antd"
  ],
  "author": "jinke.li <1359518268@qq.com>",
  "license": "MIT",
  "dependencies": {
    "antd": "latest",
    "axios": "^0.18.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.3.1",
    "core-js": "^3.1.2",
    "history": "^4.7.2",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
